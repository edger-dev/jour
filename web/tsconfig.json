{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@convex/*": [
        "convex/*"
      ],
      "@convex-svelte": [
        "convex-svelte/index"
      ],
      "@islands/*": [
        "src/islands/*"
      ],
      "@views/*": [
        "src/views/*"
      ],
      "@app/*": [
        "src/app/*"
      ],
      "@md/*": [
        "src/md/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}